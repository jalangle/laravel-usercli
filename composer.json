{
    "name": "jalangle/laravel-usercli",
    "description": "Laravel UserCLI ",
    "keywords": [
        "eloquent",
        "laravel",
        "users"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Jacob Langley",
            "email": "opensource@ghtns.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "GHTNS\\LaraUserCLI\\": "src/"
        }
    },
    "autoload-dev": {
        "classmap": ["tests"]
    },
    "require": {
        "illuminate/auth": "5.*|^6.0",
        "illuminate/console": "5.*|^6.0",
        "illuminate/container": "5.*|^6.0",
        "illuminate/contracts": "5.*|^6.0",
        "illuminate/database": "5.*|^6.0"
    },
    "require-dev": {
        "illuminate/cache": "5.*|^6.0",
        "mockery/mockery": "^0.9.5",
        "phpunit/phpunit": "^4.8.35"
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "laravel": {
            "providers": [
                "GHTNS\\LaraUserCLI\\LaraUserServiceProvider"
            ]
        }
    }
}
